<% if @comment -%>
## <%= @comment %>
<% end -%>
<% if @mailto -%>
MAILTO=<%= @mailto %>
<% end -%>

# Environment Settings
PATH=<%= @path %>
<% if @environment -%>
<% @environment.each do |key, value| -%>
<%= key %>=<%= value %>
<% end -%>
<% end -%>

# Job Definition
<%= @time %>   <%= @user %>  <%= @command %>
